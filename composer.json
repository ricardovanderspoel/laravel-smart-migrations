{
    "name": "ricardovanderspoel/laravel-smart-migrations",
    "description": "A description of your package",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.10",
        "openai-php/client": "^0.8.2",
        "symfony/http-client": "^6.4",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "RicardoVanDerSpoel\\LaravelSmartMigrations\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RicardoVanDerSpoel\\LaravelSmartMigrations\\Providers\\SmartMigrationServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
